%script{type: "text/template", id: "game-result-template"}
  %article.gameList
    %figure.gameImgWrap
      %img.gameImg{src: "/game_photos/{{= photo_path }}", width: "121", alt: ""}
    %ul.starList
      - 4.times do |i|
        %li.starOn ★
      %li.starOff ☆
    %ul.btnList
      %li.item
        %select.status{name: "status", size: 1}
          %option{value: ""} ステータス
          %option{value: 1} 気になる
          %option{value: 2} プレイ中
          %option{value: 3} アーカイブ