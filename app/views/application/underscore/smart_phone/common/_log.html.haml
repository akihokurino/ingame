%script{type: "text/template", id: "log-template"}
  %a.articleWrap{href: "{{= current_url }}", style: "display: block;"}
    %figure.game
      {{ if (game.photo_path) { }}
      %img.gameImg{src: "/game_photos/{{= game.photo_path }}"}
      {{ } else if (game.photo_url) { }}
      %img.gameImg{src: "{{= game.photo_url }}"}
      {{ } else { }}
      %img.gameImg{src: "/game_photos/default.png"}
      {{ } }}
      .gameTitle
        {{= game.title }}